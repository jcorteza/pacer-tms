{{!-- Table for adding new product information --}}
<div class="container col-md-6 offset-md-3">
  <h2 class="text-center">Insert new batch information</h2>
  <form>
    <div class="form-group">
      <label for="materialInput">Material</label>
      <select class="form-control" id="material">
        <option selected>Select material</option>
        <option>H40</option>
        <option>J55</option>
        <option>K55</option>
        <option>N80-1</option>
        <option>L80</option>
      </select>
    </div>
    <div class="form-group">
      <label for="finishInput">Finish</label>
      <select class="form-control" id="finish">
        <option selected>Select finish</option>
        <option>P-B</option>
        <option>P-P</option>
        <option>B-B</option>
        <option>PE-PE</option>
        <option>P</option>
        <option>B</option>
      </select>
    </div>
    <div class="form-group">
      <label for="rangeInput">Range</label>
      <select class="form-control" id="range">
        <option selected>Select range</option>
        <option>TYPE-2</option>
        <option>TYPE-3</option>
      </select>
    </div>
    <div class="form-group">
      <label for="quantityInput">Quantity</label>
      <input type="" class="form-control" id="quantity" placeholder="Number in Batch">
    </div>
    <div class="form-group">
      <label for="locationInput">Location</label>
      <input type="" class="form-control" id="location" placeholder="Location in Warehouse">
    </div>
  </form>
</div>

